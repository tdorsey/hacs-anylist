  "author": "Home Assistant AnyList Integration",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
